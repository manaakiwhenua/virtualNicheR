citHeader("To cite virtualNicheR in publications please use:")

citEntry(entry = "Article",
  title        = "{virtualNicheR}: generating virtual fundamental andrealised niches for use in virtual ecology experiments",
  author       = personList(as.person("Thomas R. Etherington"),
                            as.person("O. Pascal Omondiagbe")),
  journal      = "Journal of Open Source Software",
  year         = "2019",
  volume       = "4",
  number       = "41",
  pages        = "1661",
  url          = "https://joss.theoj.org/papers/10.21105/joss.01661",
         
  textVersion  = 
  paste("Etherington TR, Omondiagbe OP (2019).", 
        "virtualNicheR: generating virtual fundamental andrealised niches for use in virtual ecology experiments",
        "Journal of Open Source Software, 4(41), 1661",
		"URL https://joss.theoj.org/papers/10.21105/joss.01661.")
)
